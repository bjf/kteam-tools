#!/bin/bash

here=`dirname $0`
case "$here" in
/*) ;;
*)  here="`pwd`/$here" ;;
esac

cd "$here" || exit 1

echo "Starting `date`"
exec ./mainline-worker-kmsgq \
	--command cod-source \
	--command cod-ubuntu-config \
	--command mainline-build \
	--command mainline-flush \
	`hostname` mainline-todo \
	"$@"
