#!/bin/bash

here=`dirname $0`
case "$here" in
/*) ;;
*)  here="`pwd`/$here" ;;
esac

cd "$here" || exit 1

echo "Starting `date`"
exec ./mainline-worker-kmsgq \
	--prefix-arg ./mainline-worker-kmsgq-runner \
	--admin mainline-cleaner \
	--admin mainline-flush \
	--command cod-source \
	--command cod-ubuntu-config \
	--command mainline-build \
	--command cod-mainline-native-source \
	--command cod-changeset-build \
	`hostname` mainline-todo \
	"$@"
